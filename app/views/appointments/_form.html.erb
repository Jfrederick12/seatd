<%= form_for(:appointment, :url => employee_appointments_path) do |f| %>

  <div class="field">
    <%= f.label :booked %><br>
    <%= f.datetime_select :start_time %>
  </div>
  <div class="field">
    <%= f.label :end_time %><br>
    <%= f.datetime_select :end_time %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
